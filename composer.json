{
    "name": "sirgrimorum/JSLocalization",
    "type": "library",
    "description": "Put localization translations in js",
    "keywords": [
        "sirgrimorum",
        "Localization",
        "JavaScript"
    ],
    "homepage": "https://desarrollo.grimorum.com/andres/JSLocalization",
    "license": "MIT",
    "authors": [
        {
            "name": ":Sirgrimorum",
            "email": "andres.espinosa@grimorum.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3"
    },
    "autoload": {
        "psr-4": {
            "sirgrimorum\\JSLocalization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        },
        "laravel": {
            "providers": [
                "Sirgrimorum\\JSLocalization\\JSLocalizationServiceProvider"
            ],
            "aliases": {
                "JSLocalization": "Sirgrimorum\\JSLocalization\\JSLocalizationFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    }
}
